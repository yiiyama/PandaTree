double get_ecf(int o_, int N_, int ib_) const
{
  if (o_<1 || o_>3 || N_<1 || N_>4 || ib_<0 || ib_>3) 
    return -1.;
  return ecfs[o_-1][N_-1][ib_];
}
bool set_ecf(int o_, int N_, int ib_, float x_)
{
  if (o_<1 || o_>3 || N_<1 || N_>4 || ib_<0 || ib_>3) 
    return false;
  ecfs[o_-1][N_-1][ib_] = x_;
  return true;
}

